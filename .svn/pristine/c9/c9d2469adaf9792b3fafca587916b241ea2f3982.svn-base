html,img{margin: 0;padding: 0;}
body{background: #eeeeee;}
.header{width: 100%;background-color: rgba(0,71,193,0.7); min-height: 6rem; max-height: 12rem;position: relative;z-index: 2;}
.header-logo{padding-top: 2rem;}
.header-nav{background-color: transparent;}
.header-nav-item{line-height: 6rem !important;}
.login-vz{text-align: right;padding-right: 1rem;color: white;margin-top: 0.5rem;font-size: 0.8rem;}
.login-vz a{color: white;}
.login-vz a:hover{color: white;text-decoration: underline;}
.container{width: 100%;position: relative;}
.footer{background: #282828;min-height: 14rem;color: white;text-align: center;font-size: 1rem;}
.footer a{color: white;}
.footer a:hover{color: #ffd800;}
.footer .link{padding-top: 3rem;}
.footer .air{padding-top: 1rem;}
.footer .copyright{padding-top: 3rem;}